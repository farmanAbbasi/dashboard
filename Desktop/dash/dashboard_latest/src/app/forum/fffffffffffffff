 setMultipleChoicePoll() {

    if (this.checked == false) {
      this.checked=true;
      this.checkedBool = false;
     // this.pollShareFlagMultiple = true;
      console.log(this.checkedBool);
    }
    else {
    this.checked =false;
      this.checkedBool = true;
      console.log(this.checkedBool);
    }
  }